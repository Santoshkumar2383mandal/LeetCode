class Solution {
  //Array elements are already sorted
    public int removeDuplicates(int[] nums) {
        int c=1;
        int in=nums[0];
           for(int i=1;i<nums.length;i++)
           {
               if(nums[i]>nums[i-1])
               {
               nums[c]=nums[i];
               c++;
               }
               

              
           }
           return c;
    }

    
}
