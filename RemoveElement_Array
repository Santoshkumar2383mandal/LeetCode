class Solution {
    public int removeElement(int[] nums, int val) {
        int c=0;
        int ans=0;
        for(int i=0;i<nums.length;i++)
        {
          //////////add elements in array which are not equal to val
            if(nums[i]!=val)
            {
            nums[ans]=nums[i];
            ans++;
            }
            
        }
        
        return ans;
    }
}
